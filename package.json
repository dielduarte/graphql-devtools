{
  "name": "graphql-devtools",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@xstate/react": "^0.8.1",
    "classnames": "^2.2.6",
    "graphql": "^14.6.0",
    "graphql.js": "^0.6.6",
    "object-path-immutable": "^4.1.0",
    "prettier": "^2.0.5",
    "prismjs": "^1.19.0",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.4.1",
    "rxjs": "^6.5.3",
    "typescript": "^3.7.2",
    "xstate": "^4.7.1"
  },
  "scripts": {
    "start": "./scripts/watch.js",
    "build": "SKIP_PREFLIGHT_CHECK=true INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-actions": "^5.2.7",
    "@storybook/addon-links": "^5.2.7",
    "@storybook/addons": "^5.2.7",
    "@storybook/react": "^5.2.7",
    "@types/prettier": "^2.0.0",
    "@types/prismjs": "^1.16.0",
    "babel-loader": "^8.0.6",
    "commitlint": "^8.3.5",
    "cz-conventional-changelog": "3.2.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "webpack-extension-reloader": "^1.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
